<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <defaults public="true"/>

        <!-- Ruleset -->
        <service id="tests.skyboundtech.sylius_wholesale_plugin.behat.context.setup.ruleset_context"
                 class="Tests\SkyBoundTech\SyliusWholesalePlugin\Behat\Context\Setup\RulesetContext">
            <argument type="service" id="sylius.behat.shared_storage"/>
            <argument type="service" id="skyboundtech.repository.wholesale_ruleset"/>
            <argument type="service" id="skyboundtech.factory.wholesale_ruleset"/>
        </service>
    </services>
</container>
