<?xml version="1.0" encoding="UTF-8" ?>

<container xmlns="http://symfony.com/schema/dic/services" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <defaults public="true"/>
        <!-- Put your Behat services here -->

        <service
                id="tests.skyboundtech.sylius_wholesale_plugin.behat.page.admin.ruleset.create_page"
                class="Tests\SkyBoundTech\SyliusWholesalePlugin\Behat\Page\Admin\Ruleset\CreatePage"
                parent="sylius.behat.page.admin.crud.create"
                public="false"
        >
            <argument type="string">skyboundtech_admin_wholesale_ruleset_create</argument>
        </service>

        <service
                id="tests.skyboundtech.sylius_wholesale_plugin.behat.page.admin.ruleset.index_page"
                class="Tests\SkyBoundTech\SyliusWholesalePlugin\Behat\Page\Admin\Ruleset\IndexPage"
                parent="sylius.behat.page.admin.crud.index"
                public="false"
        >
            <argument type="string">skyboundtech_admin_wholesale_ruleset_index</argument>
        </service>

        <service
                id="tests.skyboundtech.sylius_wholesale_plugin.behat.page.admin.ruleset.update_page"
                class="Tests\SkyBoundTech\SyliusWholesalePlugin\Behat\Page\Admin\Ruleset\UpdatePage"
                parent="sylius.behat.page.admin.crud.update"
                public="false"
        >
            <argument type="string">skyboundtech_admin_wholesale_ruleset_update</argument>
        </service>
    </services>
</container>
